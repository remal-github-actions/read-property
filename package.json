{
  "$schema": "https://json.schemastore.org/package",
  "name": "read-property",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --fix src/**/*",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run lint && rimraf --glob build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir src --outDir build",
    "dist": "npm run compile && rimraf --glob dist && ncc build build/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "java-props": "2.3.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.10",
    "@types/semver": "^7",
    "@vercel/ncc": "0.38.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-github": "5.1.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-extended": "4.0.2",
    "rimraf": "6.0.1",
    "semver": "^7",
    "ts-jest": "29.2.5",
    "typescript": "5.7.2",
    "typescript-eslint": "8.19.0"
  }
}
